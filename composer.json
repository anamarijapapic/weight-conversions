{
    "name": "anamarijapapic/weight-conversions",
    "description": "Weight conversions in PHP.",
    "keywords": [
        "anamarijapapic",
        "weight-conversions"
    ],
    "homepage": "https://github.com/anamarijapapic/weight-conversions",
    "license": "MIT",
    "authors": [
        {
            "name": "Anamarija PapiÄ‡",
            "email": "anamarija.papic@agilo.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "friendsofphp/php-cs-fixer": "^3.13",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "AnamarijaPapic\\WeightConversions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnamarijaPapic\\WeightConversions\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
